<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="611.0" prefWidth="352.0"
            xmlns="http://javafx.com/javafx/9.0.1" fx:controller="main.controllers.editors.PersonalEditController">
    <children>
        <VBox layoutX="251.0" layoutY="166.0" prefHeight="610.0" prefWidth="463.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Personal">
                    <font>
                        <Font name="Microsoft JhengHei Bold" size="64.0"/>
                    </font>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Text>
                <JFXTextField fx:id="companyField" labelFloat="true" promptText="Company">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="phoneField" labelFloat="true" layoutX="10.0" layoutY="116.0" promptText="Phone">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="addressField" labelFloat="true" layoutX="10.0" layoutY="141.0"
                              promptText="Address">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="trnField" labelFloat="true" layoutX="10.0" layoutY="166.0" promptText="TRN">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="emailField" labelFloat="true" layoutX="10.0" layoutY="191.0" promptText="E-Mail">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="idField" disable="true" editable="false" labelFloat="true" layoutX="10.0"
                              layoutY="216.0" promptText="ID">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextField>
                <JFXTextArea fx:id="notesField" labelFloat="true" prefHeight="164.0" prefWidth="332.0"
                             promptText="Notes">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXTextArea>
                <JFXComboBox fx:id="typeComboBox" labelFloat="true" prefHeight="25.0" prefWidth="493.0"
                             promptText="Type">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </JFXComboBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteAction"
                                        text="Delete">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" layoutX="29.0" layoutY="10.0" prefHeight="100.0"
                              prefWidth="200.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Button defaultButton="true" mnemonicParsing="false" onAction="#saveAction" text="Save">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </Button>
                                <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelAction"
                                        text="Cancel">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
